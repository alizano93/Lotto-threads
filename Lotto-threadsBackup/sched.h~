

#ifndef __SCHED_H__
#define __SCHED_H__
enum{EXPROPIATIVO, NO_EXPROPIATIVO};
/* scheduler struct */
struct sched_t {
    void (*addTask)(Thread *);
    void (*removeTask)(int);
    void (*manageTimer)();
    void (*init)(int, int);
    
};

/* scheduler alloc functions */
struct sched_t *sched_ls_alloc();
struct sched_t *sched_fifo_alloc();

#endif// __CO_SCHED_H__
