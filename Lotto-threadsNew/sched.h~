

#ifndef __SCHED_H__
#define __SCHED_H__

#include "Structures.h"
#include <ucontext.h>
enum{EXPROPIATIVO, NO_EXPROPIATIVO};
/* scheduler struct */
struct sched_t {
    void (*addTask)(struct task_t*, ucontext_t);
    void (*removeTask)(int);
    void (*manageTimer)();
    void (*init)(int, int);
    void (*updateWork)(float, double);
    
};

/* scheduler alloc functions */
struct sched_t *sched_ls_alloc();
struct sched_t *sched_fifo_alloc();

#endif// __CO_SCHED_H__
